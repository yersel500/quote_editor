<%= turbo_stream_from "quotes" %>
<main class='container'>
  <%= button_to(
  "Log Out",
  destroy_user_session_path,
  method: :delete
) %>
  <div class='header'>
    <h1>Quotes</h1>
    <%= link_to 'New quote',
              new_quote_path,
              class: 'btn btn--primary',
              data: { turbo_frame: dom_id(Quote.new) }
              %>
  </div>
  <%= turbo_frame_tag Quote.new %>
  <%= turbo_frame_tag "quotes" do %>
    <%= render @quotes%>
  <% end %>
</main>
